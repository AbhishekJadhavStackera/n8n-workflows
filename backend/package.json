{
  "name": "n8n-workflows-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "build": "tsc",
    "start": "npm run push && node dist/index.js",
    "migrate": "prisma migrate dev --name init",
    "generate": "prisma generate",
    "push": "prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.2",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "pino": "^9.9.0",
    "prisma": "^6.15.0",
    "tslib": "^2.8.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
